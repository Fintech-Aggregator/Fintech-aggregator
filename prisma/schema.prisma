generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id        Int      @id @default(autoincrement())
  fullName  String
  email     String   @unique
  password  String
  token     String
  role      UserRole @default(USER)
  createdAt DateTime @default(now())
}

model HongKong {
  id          Int    @id @default(autoincrement())
  licenseName String
  address     String
  addressType String
}

model EMoneyFirms {
  FRN                       Int    @id
  FirmName                  String
  EmoneyRegisterStatus      String
  EmoneyStatusEffectiveDate String
}

model FirmPSDPermission {
  FRN                    Int    @id
  FirmName               String
  PSDFirmStatus          String
  PSDStatusEffectiveDate String
}

model Lithuania {
  id          Int    @id @default(autoincrement())
  FirmName    String
  Address     String
  Site        String
  Licence     String
  Date        String
  LicenceLink String
  CompanyLink String
}

model Lithuania {
    id Int @id @default(autoincrement())
    FirmName String
    Address String
    Site String
    Licence String
    Date String
    LicenceLink String
    CompanyLink String
}

enum UserRole {
  USER
  ADMIN
}
